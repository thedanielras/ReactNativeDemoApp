{
  "expo": {
    "name": "ReactNativeDemoApp",
    "slug": "ReactNativeDemoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "description": "An app written in react native to experiment and learn, fell free to contribute.",
    "githubUrl": "https://github.com/thedanielras/ReactNativeDemoApp",
    "android": {
      "package": "com.thedanielras.ReactNativeDemoApp"
    }
  }
}
